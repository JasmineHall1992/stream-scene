{
  "name": "stream-scene",
  "version": "1.0.0",
  "main": "server/app.ts",
  "scripts": {
    "dev": "npm run build:client && npm run dev:server",
    "dev:client": "webpack --config webpack.config.mjs --mode development",
    "dev:server": "nodemon --watch server --ext ts --exec ts-node server/app.ts",
    "build": "webpack --config webpack.config.mjs --mode production && tsc",
    "build:client": "webpack --config webpack.config.mjs --mode development",
    "start": "node dist/server/app.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/s3-request-presigner": "^3.859.0",
    "@emotion/is-prop-valid": "^1.3.1",
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "framer-motion": "^12.23.12",
    "mysql2": "^3.14.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1",
    "react-sketch-canvas": "^6.2.0",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/express-session": "^1.18.2",
    "@types/node": "^24.1.0",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/sequelize": "^4.28.20",
    "autoprefixer": "^10.4.21",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.0",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.1.1",
    "html-webpack-plugin": "^5.6.3",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^4.1.11",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "util": "^0.12.5",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
